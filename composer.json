{
    "name": "sormagec/laravel-ffmpeg",
    "description": "FFMpeg for Laravel",
    "keywords": [
        "sormagec",
        "laravel-ffmpeg"
    ],
    "homepage": "https://github.com/sormagec/laravel-ffmpeg",
    "license": "MIT",
    "authors": [
        {
            "name": "Burak Sormage√ß",
            "email": "burak@sormagec.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/config": "5.5.*",
        "illuminate/filesystem": "5.5.*",
        "illuminate/log": "5.5.*",
        "league/flysystem": "~1.0",
        "sormagec/php-ffmpeg": "~0.14.0",
        "symfony/process": "^4.0|^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Pbmedia\\LaravelFFMpeg\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pbmedia\\LaravelFFMpeg\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pbmedia\\LaravelFFMpeg\\FFMpegServiceProvider"
            ],
            "aliases": {
                "FFMpeg": "Pbmedia\\LaravelFFMpeg\\FFMpegFacade"
            }
        }
    }
}
